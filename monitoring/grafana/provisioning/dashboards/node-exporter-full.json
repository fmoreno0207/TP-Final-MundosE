{
  "id": null,
  "uid": "tech-store-full",
  "title": "TechStore - Full App Monitoring",
  "tags": ["techstore", "app", "nodejs"],
  "timezone": "browser",
  "schemaVersion": 36,
  "version": 1,
  "refresh": "5s",
  "panels": [
    {
      "type": "stat",
      "title": "CPU User (s)",
      "gridPos": { "x": 0, "y": 0, "w": 8, "h": 4 },
      "targets": [
        {
          "expr": "TechStore_process_cpu_user_seconds_total",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "CPU System (s)",
      "gridPos": { "x": 8, "y": 0, "w": 8, "h": 4 },
      "targets": [
        {
          "expr": "TechStore_process_cpu_system_seconds_total",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "RAM (RSS bytes)",
      "gridPos": { "x": 16, "y": 0, "w": 8, "h": 4 },
      "targets": [
        {
          "expr": "TechStore_process_resident_memory_bytes",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Virtual Memory (bytes)",
      "gridPos": { "x": 0, "y": 4, "w": 8, "h": 4 },
      "targets": [
        {
          "expr": "TechStore_process_virtual_memory_bytes",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Event Loop Lag (seconds)",
      "gridPos": { "x": 8, "y": 4, "w": 16, "h": 8 },
      "targets": [
        {
          "expr": "TechStore_nodejs_eventloop_lag_seconds",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Heap Usage (bytes)",
      "gridPos": { "x": 0, "y": 12, "w": 24, "h": 8 },
      "targets": [
        {
          "expr": "TechStore_process_heap_bytes",
          "refId": "A"
        }
      ]
    }
  ]
}
